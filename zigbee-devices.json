                {
                  "name": "contact",
                  "icon": {
                    "value": "fas fa-door-closed",
                    "thresholds": [
                      {
                        "op": "eq",
                        "value": "true|1",
                        "target": "fas fa-door-open"
                      }
                    ]
                  },
                  "valueConverter": "return value == true ? 'opened' : 'closed';",
                  "iconColor": {
                    "thresholds": [
                      {
                        "op": "eq",
                        "value": "true|1",
                        "target": "#3A75B5"
                      }
